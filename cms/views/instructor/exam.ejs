<body>


<!-- header -->
<header>
    <div class="container">

        <!-- nav -->
        <nav class="navbar navbar-expand-lg navbar-light py-sm-4 py-2">
            <!-- logo -->
            <h1>
                <a class="navbar-brand" href="/student/"> <i class="fab fa-affiliatetheme"></i>
                    Eduversity
                </a>
            </h1>
            <!-- //logo -->
            <button class="navbar-toggler ml-md-auto" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <!-- main nav -->
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ml-lg-auto text-center">
                    <li class="nav-item mr-lg-3">
                        <a class="nav-link" href="/student/">Home
                            <span class="sr-only">(current)</span>
                        </a>
                    </li>
                    <li class="nav-item mr-lg-3">
                        <a class="nav-link" href="/student/courses/">Courses</a>
                    </li>
                    <li class="dropdown nav-item active mr-lg-3">
                        <a href="#" data-toggle="dropdown" class="nav-link dropdown-toggle ">
                            Account
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu" role="menu">
                            <li class="nav-item">
                                <a href="/student/profile" class="nav-link">Profile</a>
                            </li>
                            <li class="nav-item">
                                <a href="/users/logout" class="nav-link">Logout</a>
                            </li>
                        </ul>
                    <li>
                        <!-- search --->
                        <div class="search-bar-agileits">
                            <div class="cd-main-header">
                                <ul class="cd-header-buttons">
                                    <li><a class="cd-search-trigger" href="#cd-search"> <span></span></a></li>
                                </ul>
                                <!-- cd-header-buttons -->
                            </div>
                            <div id="cd-search" class="cd-search">
                                <form action="#" method="post">
                                    <input name="Search" type="search" placeholder="Click enter after typing...">
                                </form>
                            </div>
                        </div>
                        <!-- search --->

                    </li>
                </ul>
            </div>
            <!-- //main nav -->
        </nav>
        <!-- //nav -->
    </div>
</header>
<!-- //header -->
<% if (messages) { %>
<%if(messages[0]){%>
    <div class="alert alert-success">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        <i class="fa sign"></i><strong></strong>
        <span><%= messages[0]%></span>
    </div>
<%}%>
<% } %>
<!-- inner banner -->
<div class="inner_banner layer" id="home">
    <div class="container">
        <div class="agileinfo-inner">
            <h2 class="text-center text-white">
                <%= course.Course_Title%>
            </h2>
        </div>
    </div>
</div>
<!-- //inner banner -->
<!-- breadcrumbs -->
<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a href="/instructor/">Home</a>
        </li>
        <li class="breadcrumb-item">
            <a href="/instructor/courses/">Courses</a>
        </li>
        <li class="breadcrumb-item">
            <a href="/instructor/course-details/<%= course.Course_id%>"><%= course.Course_Title%></a>
        </li>
        <li class="breadcrumb-item active" aria-current="page">Exam</li>
    </ol>
</nav>
<!-- //breadcrumbs -->

<!-- courses -->
<section class="courses py-5">





    <!-- single page -->
    <div class="single-page py-5">
        <div class="container py-xl-5 py-lg-3">
            <div class="inner-sec">
                <div class="row">
                    <!-- left side -->
                    <div class="col-lg-9 left-blog-info text-left">
                        <div class="blog-grid-top">

                            <h2 class="tittle mb-xl-5 mb-4 text-dark text-uppercase font-weight-bold pl-4 py-3"></h2>

                            <div class="category-story tech-btm"">
                            <ul class="list-group single" >

                                    <% if (questions) { %>
                                        <% for (var i=0;i<questions.length ;i ++) {%>
                                            <div class="card ">


                                            <div class="card-header" >
                                                <h3><%= i+1 %>. &nbsp <%= questions[i].Question%></h3>
                                              </div>

                                         <div class="card-body" style="margin-left:8%">

                                            <ul>
                                                <li><%= questions[i].Op1%></li><br>
                                                <li><%= questions[i].Op2%></li><br>
                                                <li><%= questions[i].Op3%></li><br>
                                                <li><%= questions[i].Op4%></li><br>
                                            </ul>
                                        </div>

                                            <div class="card-footer"> Correct Option :  <%= questions[i].Correct_Option%></div>
</div>
                                            <hr>
                                        <% }%>
                                    <% } %>




                            </ul>
                        </div>









                    </div>


                </div>
                <!-- //left side -->
                <div class="col-lg-3 left-blog-info text-left">

                    <div class="category-story tech-btm"">
                    <ul class="list-group single" >

                                    <h3>View Submissions</h3>
                        <li class="list-group-item d-flex justify-content-between align-items-center" >
                            <a style="color: red">First Name</a><br><br><br>
                            <a style="color: red">Last Name</a><br><br><br>
                            <a style="color: red">Roll Number</a><br><br><br>



                        </li>



                        <% if (students) { %>
                            <% for (var i=0;i<students.length ;i ++) {%>
                                <li class="list-group-item d-flex justify-content-between align-items-center" >
                                    <a><%= students[i].First_Name%></a>
                                    <a><%= students[i].Last_Name%></a>
                                    <a href="/instructor/course/<%= course.Course_id %>/exams/<%= exam.Exam_id%>/<%= students[i].Student_id%>" ><%= students[i].Roll_Number%></a>




                                </li>
                            <% }%>
                        <% } %>



                    </ul>
                </div>
                </div>

            </div>
        </div>
    </div>
    </div>










</section>
<!-- courses -->





<!-- js -->
<script type="text/javascript" src="/js/jquery-2.1.4.min.js"></script>
<script type="text/javascript" src="/js/bootstrap.js"></script> <!-- Necessary-JavaScript-File-For-Bootstrap -->
