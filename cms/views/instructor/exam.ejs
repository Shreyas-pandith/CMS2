<script>
    $("#formButton").click(function(){
        $("#introduction_textarea").toggle();
        $("#introduction").css({
            'display': 'none'
        });
    });

    $("#formButton2").click(function(){
        $("#description_textarea").toggle();
        $("#description").css({
            'display': 'none'
        });
    });


    $("#formButton3").click(function(){
        $("#requirements_textarea").toggle();
        $("#requirements").css({
            'display': 'none'
        });
    });
</script>

